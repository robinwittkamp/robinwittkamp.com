{
  "useTabs": false,
  "semi": true,
  "printWidth": 100,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["*.html", "*.php"],
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.js", "*jsx", "*.ts", "*tsx"],
      "options": {
        "singleQuote": true
      }
    }
  ]
}
